{
  "name": "@h3ravel/collect.js",
  "version": "5.0.5",
  "description": "Convenient and dependency free wrapper for working with arrays and objects (maintained).",
  "typings": "index.d.ts",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://collect.js.toneflix.net",
  "repository": {
    "type": "git",
    "url": "https://github.com/h3ravel/collect.js"
  },
  "scripts": {
    "all": "pnpm run barrel && pnpm run build && pnpm test --watch=false && pnpm run eslint && pnpm run validate-version",
    "lint": "eslint . --ext .ts --ext .js",
    "pretest": "pnpm run transpile",
    "test": "vitest",
    "barrel": "barrelize >/dev/null",
    "coverage": "vitest --coverage",
    "transpile": "tsdown --config-loader unconfig",
    "readme": "node documentor/bundle.js",
    "build": "pnpm run transpile && pnpm run readme",
    "eslint": "pnpm eslint:fix",
    "eslint:fix": "pnpm eslint:js --fix && pnpm run eslint:md --fix",
    "eslint:strict": "pnpm run eslint:js && pnpm run eslint:md",
    "eslint:js": "eslint src/ tests/",
    "eslint:md": "eslint --ext md README.md docs/ --rule strict:off --rule no-undef:off --rule no-unused-vars:off --rule func-names:off --rule no-console:off --rule consistent-return:off --rule import/no-unresolved:off --rule import/extensions:off --parser-options sourceType:module",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "validate-version": "echo 1 >/dev/null",
    "prepublishOnly": "pnpm run all",
    "docs:preview": "vitepress preview docs",
    "release:patch": "pnpm build && pnpm version patch && git add . && git commit -m \"version: bump console package version\" && pnpm publish --tag latest",
    "version-patch": "pnpm version patch"
  },
  "keywords": [
    "h3ravel",
    "maintained",
    "latest",
    "collections",
    "collection",
    "collect",
    "map",
    "filter",
    "group by",
    "reduce",
    "transform",
    "pluck",
    "pie",
    "mapWithKeys",
    "laravel",
    "whereIn",
    "values",
    "unique"
  ],
  "author": {
    "name": "Daniel Eckermann",
    "email": "danieleckermann@gmail.com",
    "url": "https://danieleckermann.com"
  },
  "contributors": [
    {
      "name": "Daniel Eckermann",
      "email": "danieleckermann@gmail.com",
      "url": "https://danieleckermann.com"
    },
    {
      "name": "3m1n3nc3",
      "email": "hamzas.legacy@toneflix.net",
      "url": "https://hamzas.legacy.toneflix.net"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/h3ravel/collect.js/issues"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@eslint/markdown": "^7.5.1",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitest/coverage-v8": "4.0.16",
    "barrelize": "^1.6.4",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "tsdown": "^0.16.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.50.0",
    "vitepress": "2.0.0-alpha.15",
    "vitest": "^4.0.16",
    "vue": "^3.5.26"
  },
  "packageManager": "pnpm@10.18.2"
}